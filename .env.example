# EC2 Configuration (CENTRAL - Update IP here for entire codebase)
EC2_HOST=your-ec2-ip-address
EC2_SSH_KEY_PATH=./your-key.pem
EC2_SSH_USER=ubuntu

# EC2 Database Configuration
POSTGRES_HOST=${EC2_HOST}
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-database-password
POSTGRES_DB=munbon_dev

# TimescaleDB Configuration (on same EC2)
TIMESCALE_HOST=${EC2_HOST}
TIMESCALE_PORT=5432
TIMESCALE_USER=postgres
TIMESCALE_PASSWORD=your-database-password
TIMESCALE_DB=sensor_data

# Database URLs (for convenience)
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${EC2_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}
TIMESCALE_URL=postgresql://${TIMESCALE_USER}:${TIMESCALE_PASSWORD}@${EC2_HOST}:${TIMESCALE_PORT}/${TIMESCALE_DB}

# AWS Configuration
AWS_REGION=ap-southeast-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key

# Application Configuration
NODE_ENV=development
PORT=3000

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password

# JWT Configuration
JWT_SECRET=your-jwt-secret
JWT_EXPIRATION=7d

# External Services
MSSQL_HOST=your-mssql-host
MSSQL_PORT=1433
MSSQL_DATABASE=your-mssql-database
MSSQL_USER=your-mssql-user
MSSQL_PASSWORD=your-mssql-password

# API Keys
API_KEY=your-api-key


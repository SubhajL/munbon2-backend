{
  "name": "@munbon/sensor-data-service",
  "version": "1.0.0",
  "description": "Sensor data service for Munbon irrigation system",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/cmd/server/main.js",
    "dev": "nodemon --watch src --exec ts-node src/cmd/server/main.ts",
    "consumer": "ts-node src/cmd/consumer/main.ts",
    "consumer:dev": "nodemon --watch src --exec ts-node src/cmd/consumer/main.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "test:send": "node test-send-data.js",
    "check:data": "node check-data.js",
    "check:sqs": "node check-sqs.js"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.451.0",
    "@types/geojson": "^7946.0.13",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "mqtt": "^5.3.0",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "socket.io": "^4.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.15.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}

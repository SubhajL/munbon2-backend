{
  "name": "@munbon/rid-ms-service",
  "version": "1.0.0",
  "description": "RID-MS Service for processing SHAPE files from Royal Irrigation Department",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:processor": "node dist/processor.js",
    "dev": "nodemon --exec ts-node src/index.js",
    "dev:processor": "nodemon --exec ts-node src/processor.ts",
    "build:lambda": "tsc && cp package*.json dist/",
    "deploy": "npm run build:lambda && serverless deploy --config serverless-simple.yml",
    "deploy:function": "npm run build:lambda && serverless deploy function -f",
    "deploy:quick": "npm install -g serverless && npm run deploy",
    "remove": "serverless remove --config serverless-simple.yml",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist coverage .serverless"
  },
  "keywords": [
    "rid",
    "shapefile",
    "gis",
    "water-demand",
    "irrigation"
  ],
  "author": "Munbon Irrigation Project",
  "license": "ISC",
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "@types/geojson": "^7946.0.13",
    "adm-zip": "^0.5.10",
    "aws-sdk": "^2.1500.0",
    "bull": "^4.11.5",
    "cors": "^2.8.5",
    "dbffile": "^1.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.2.0",
    "geojson": "^0.5.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "proj4": "^2.9.2",
    "shapefile": "^0.6.6",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/proj4": "^2.5.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts"
    ]
  }
}
